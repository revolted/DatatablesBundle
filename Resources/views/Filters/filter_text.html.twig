<input type="text"
       class="individual_filtering{% if column.filterClass is not null %} {{ column.filterClass }} {% endif %}"
       style="{% if column.width %} width:{{ column.width }};{% endif %}"
       placeholder="{% if column.placeholder is not null %}{{ column.placeholder }}{% else %}{{ column.title|striptags|trim }}{% endif %}"
       {% if column.name %}name="{{ column.name }}"{% endif %}
       data-filter-property-id="{{ filterColumnId }}" />
